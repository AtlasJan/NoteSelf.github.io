{
	"plugins": [
          "tiddlypouch"
	],
	"themes": [
		"tiddlywiki/vanilla",
		"tiddlywiki/snowwhite"
	],
	"languages": [
		"en-US"
	],
	"build": {
		"index": [
			"--rendertiddler","$:/NoteSelf/save/deploy","index.html","text/plain"],
        "OnlineDemo": [
			"--rendertiddler","$:/plugins/danielo515/tiddlypouch/save/basic","demo.html","text/plain"],
		"favicon": [
			"--savetiddler","$:/favicon.ico","favicon.ico"],
		"readme": [
			"--rendertiddler","ReadMe","readme.md","text/html",
			"--rendertiddler","License","license.md","text/html"]
	}
}
